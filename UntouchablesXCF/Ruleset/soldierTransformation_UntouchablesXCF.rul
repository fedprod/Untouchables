      - STR_KYBEROS
      - STR_INFERNAL
      - STR_SPARTAN
      - STR_HYBRID
      - STR_MUGGLE_AI
      - STR_DOG
      - STR_RAT
      - STR_BAT
    cost: 100000
    transferTime: 1
    recoveryTime: 28
    listOrder: 15
