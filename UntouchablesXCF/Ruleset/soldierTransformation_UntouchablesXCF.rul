      - STR_PROTEAN
      - STR_OLYMPIAN
      - STR_KYBEROS
      - STR_INFERNAL
      - STR_SPARTAN
      - STR_HYBRID
      - STR_MUGGLE_AI
      - STR_DOG
      - STR_RAT
      - STR_BAT
    cost: 20000
    recoveryTime: 1
    listOrder: 5
